LIB.namespace("ADLIB.infoBoxPopover"),function(){"use strict";function i(t){t.preventDefault();t.stopPropagation();n.find(".visible-info").removeClass("visible-info")}function r(n){n.on("click",".i-info",function(t){t.preventDefault();t.stopPropagation();var i=$(this);if(!i.hasClass(".visible-info")){n.find(".visible-info").removeClass("visible-info");i.addClass("visible-info");i.next(".info-content").addClass("visible-info");n.on("click",".info-content .popout",function(n){n.stopPropagation()})}});n.on("click",".info-content .close",i)}function t(){n=$(".js-info-box-popover");n.each(function(){r($(this))})}var n=$(".js-info-box-popover");ADLIB.infoBoxPopover={infoBoxPopoverInit:function(){t()}};t()}()